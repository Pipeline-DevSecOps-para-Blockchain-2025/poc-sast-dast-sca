{
  "detectors_to_run": [
    "all"
  ],
  "detectors_to_exclude": [
    "pragma",
    "solc-version",
    "low-level-calls"
  ],
  "disable_color": false,
  "filter_paths": [
    "node_modules",
    "lib",
    "test"
  ],
  "exclude_informational": false,
  "exclude_low": false,
  "exclude_medium": false,
  "exclude_high": false,
  "json": true,
  "sarif": false,
  "checklist": false,
  "markdown": false,
  "zip": false,
  "zip_type": "lzma",
  "show_ignored_findings": false,
  "legacy_ast": false,
  "ignore_return_value": false,
  "disable_solc_warnings": false,
  "optimize": true,
  "optimize_runs": 200,
  "ignore_compile": false,
  "fail_pedantic": false,
  "fail_low": false,
  "fail_medium": false,
  "fail_high": true,
  "no_fail": false,
  "solc_remaps": [],
  "solc_args": "--optimize --optimize-runs 200",
  "solc_working_dir": null,
  "generate_patches": false,
  "skip_assembly": false,
  "perf": false,
  "debug": false,
  "more_details": false,
  "timeout": 60000
}